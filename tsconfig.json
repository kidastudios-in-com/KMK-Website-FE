  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/Navbar.tsx", "src/components/Navbar2.jsx", "src/components/SubscriptionNew.jsx", "src/pages/screens/HomePage.jsx", "src/pages/BlogsPages/BSection2.jsx", "src/pages/screens/WhyUs.jsx", "src/pages/screens/WhyUs.jsx", "src/pages/AboutPages/Section3.jsx", "src/pages/UserDetails/UserDetails.jsx"],
  "exclude": ["node_modules"]
}
